	<params>
		<param name="toolbar" type="list" default="Advanced" label="Administrator End Toolbar Mode" description="FUNCTIONBK">
			<option value="Advanced">Advanced</option>
			<option value="Creative">Creative</option>
			<option value="Blog">Blog</option>
			<option value="Default">Default</option>
		</param>
		<param name="toolbar_ft" type="list" default="Advanced" label="FrontEnd Toolbar Mode" description="FUNCTIONFT">
			<option value="Advanced">Advanced</option>
			<option value="Creative">Creative</option>
			<option value="Blog">Blog</option>
			<option value="Default">Default</option>
		</param>
		<param name="skin" type="list" default="office2007" label="Skin" description="EDITORSKIN">
			<option value="default">default</option>
			<option value="office2007">office2007</option>
			<option value="office2003">office2003</option>
			<option value="office2003blue">office2003blue</option>
			<option value="silver">silver</option>
		</param>
		<param name="content_css" type="radio" default="1" label="Use Template CSS classes?" description="LOADCSSTMPT">
 			<option value="0">No</option>
 			<option value="1">Yes</option>
 		</param>
		<param name="editor_css" type="radio" default="0" label="Use Editor CSS classes?" description="LOADCSSED">
 			<option value="0">No</option>
 			<option value="1">Yes</option>
 		</param>
 		<param name="content_css_custom" type="text" default="" label="Use Custom CSS Classes?" description="CUSLOADCSS"/>
 		<param name="imagePath" type="text" default="/images/stories/" label="Location of the Images?" description="IMGPATH"/>
 		<param name="text_direction" type="list" default="ltr" label="Text Direction" description="TXTDIRN">
 			<option value="ltr">Left to Right</option>
 			<option value="rtl">Right to Left</option>
 		</param>
		<param name="entermode" type="list" default="0" label="Enter Mode" description="DESCENTERMODE">
			<option value="0">BR Elements</option>
 			<option value="1">P Elements</option>
 			<option value="2">Div Elements</option>
 		</param>
		<param name="shiftentermode" type="list" default="1" label="Shift Enter Mode" description="DESCSHIFTENTERMODE">
			<option value="0">BR Elements</option>
 			<option value="1">P Elements</option>
 			<option value="2">Div Elements</option>
 		</param>
		<param name="wwidth" type="text" default="100%" label="width" description="The width of Editor"/>
		<param name="hheight" type="text" default="480" label="height" description="The height of Editor"/>
	</params>
	<params group="advanced">
		<param name="showerrors" type="radio" default="1" label="Display Errors" description="DESCSHOWERROR">
 			<option value="0">No</option>
 			<option value="1">Yes</option>
 		</param>		
		<param name="lang_mode" type="radio" default="1" label="Language Selection Mode" description="DESCLANGMODE">
 			<option value="0">Manual</option>
 			<option value="1">Joomla! Default</option>
			<option value ="2">Browser Default</option>
 		</param>
 		<param name="lang_code" type="text" default="en" label="Language Code" description="DESCLANGCODE"/>
 		<param name="@spacer" type="spacer" default="" label="" description="" />
		<param name="htmlentities" type="radio" default="0" label="Process HTML Entities" description="DESCPROCHTMLENT">
 			<option value="0">No</option>
 			<option value="1">Yes</option>
 		</param>
		<param name="includelatinentities" type="radio" default="0" label="Include Latin Entities" description="DESCINCLATINENT">
 			<option value="0">No</option>
 			<option value="1">Yes</option>
 		</param>
		<param name="includegreekentities" type="radio" default="0" label="Include Greek Entities" description="DESCINCGREEKENT">
 			<option value="0">No</option>
 			<option value="1">Yes</option>
 		</param>
		<param name="numericentities" type="radio" default="0" label="Process Numeric Entities" description="DECPROCNUMENT">
 			<option value="0">No</option>
 			<option value="1">Yes</option>
 		</param>
		<param name="UserRelativeFilePath" type="radio" default="0" label="Use Relative path for Images" description="USERELPATH">
 			<option value="0">No</option>
 			<option value="1">Yes</option>
 		</param>
		<param name="FormatSource" type="radio" default="1" label="Auto Format Source OFF" description="FMTSRC">
 			<option value="1">Yes</option>
 			<option value="0">No</option>
		</param>
		<param name="add_stylesheet_path" type="text" default="" label="Location For Additional Stylesheets" description="DESCADDCSSSHEET"/>
			<param name="add_stylesheet" type="text" default="" label="Additional Stylesheets" description="DESCADDCSSHEETNAMES"/>
		<param name="bgcolor" type="text" default="#FFFFFF" label="Background Color" description="DESCBKCOLOR"/>	
	</params>
